{
    "name": "catlabinteractive/charon-laravel",
    "description": "Laravel specific libraries for Charon.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thijs Van der Schaeghe",
            "email": "info@catlab.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "CatLab\\Charon\\Laravel\\": "src/"
        }
    },
    "require": {
        "catlabinteractive/charon": "^1.3.5",
        "catlabinteractive/base-laravel": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "laravel/framework": "5.2.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CatLab\\Charon\\Laravel\\RESTResourceProvider"
            ]
        }
    }
}
